func OnKeyUp(key)
begin
	if (key == "d")
		xd := 0
	else if (key == "a")
		xd := 0
	else if (key == "w")
		yd := 0
	else if (key == "s")
		yd:= 0
end

func OnKeyDown(key)
begin
	if (key == "d")
		xd := 5
	else if (key == "a")
		xd := -5
	else if (key == "w")
		yd := -5
	else if (key == "s")
		yd:= 5
end

global x := 0
global y := 0
global xd := 0
global yd := 0

while(1)
begin
	ClearScreen()
	x := x + xd
	y := y + yd
	DrawRectangle(x, y,50,50,"red")
	Delay(1)
end
